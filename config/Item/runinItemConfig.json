{
    "UUT_MODEL": "eden",
    "station_type": "RUNIN",
    "TIME_OUT_TEST": 7000,
    "INIT": [
        {
            "FLAG": true,
            "TIME_OVER": 50,
            "SEND_FORMAT": ["sn", "pcname"],
            "DATA_FORMAT": ["mlbsn", "ethernetmac", "pnname", "model", "smode"],
            "URL": "http://10.90.0.40/sfcapi/api/connect",
            "test_name": "gen_node",
            "FUNC_NAME": "SfisFunctionProxy"
        },
        {
            "FLAG": false,
            "CANCEL": "DEBUG",
            "MULTI": true,
            "FAIL_CONTNIUE": false,
            "TIME_OVER": 1,
            "test_name": "mac",
            "FUNC_NAME": "GetMacFormSfisProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 30,
            "MULTI": false,
            "host": "192.168.1.10",
            "user": "admin",
            "password": "1234",
            "Times": 1,
            "Delay": 3,
            "Command": ["on"],
            "test_name": "wps_ok",
            "FUNC_NAME": "PowerSwitchProxy"
        }
    ],
    "Functions": [
        {
            "FLAG": true,
            "TIME_OVER": 360,
            "MULTI": false,
            "IP": "192.168.1.1",
            "test_name": "dut_ping",
            "FUNC_NAME": "DutPingProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": false,
            "IP": "192.168.1.1",
            "test_name": "dut_telnet",
            "FUNC_NAME": "DutTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": true,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "fw_printenv  serial",
            "Startkey": "serial=",
            "Endkey": "",
            "compareWith": "mblsn",
            "test_name": "sn_ok",
            "FUNC_NAME": "CheckDutInfoProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": true,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "fw_printenv  mac",
            "Startkey": "mac=",
            "Endkey": "",
            "compare": "ethernetmac",
            "test_name": "mac_ok",
            "FUNC_NAME": "CheckDutInfoProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": false,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "cat /sys/bus/i2c/devices/0-0030/name",
            "Regex": "^([a-z|A-Z]|[0-9])+$",
            "test_name": "led_vendor",
            "FUNC_NAME": "CheckCommandTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": true,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "dmesg|grep 'PHY ID'",
            "Startkey": "eth1's PHY ID is",
            "Endkey": "",
            "test_name": "phy_model",
            "FUNC_NAME": "CheckCommandTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": true,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "fwversion",
            "Startkey": "Ambit test image version:V",
            "Endkey": "",
            "test_name": "test_image_version",
            "FUNC_NAME": "CheckCommandTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": true,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "cat /sys/devices/virtual/thermal/thermal_zone1/temp",
            "Regex": "^[0-9]+$",
            "test_name": "temp_cpu_0",
            "FUNC_NAME": "CheckCommandTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 300,
            "RETRY": 3,
            "MULTI": true,
            "Times": 3,
            "test_name": "reboot_soft",
            "FUNC_NAME": "RebootSoftProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 1200,
            "MULTI": false,
            "host": "192.168.1.10",
            "user": "admin",
            "password": "1234",
            "Times": 100,
            "Delay": 5,
            "Command": ["off", "on"],
            "test_name": "power_cycle",
            "FUNC_NAME": "PowerSwitchProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 120,
            "MULTI": false,
            "IP": "192.168.1.1",
            "test_name": "dut_ping",
            "FUNC_NAME": "DutPingProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 1200,
            "MULTI": false,
            "IP": "192.168.1.1",
            "command": ["badblocks -svw /dev/mmcblk0p17", "badblocks -svw /dev/mmcblk0p20"],
            "Startkey": "Pass completed,",
            "Endkey": "bad blocks found",
            "ReadUntil": "root@eero-test:/#",
            "test_name": "emmc_badblocks",
            "FUNC_NAME": "MMC_BadBlockProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 3000,
            "MULTI": false,
            "IP": "192.168.1.1",
            "command": ["stressapptest -s 600 -M 300 -m 5 -C 5 -W -l test_ddr_01.log",
                "stressapptest -s 600 --cc_test -M 300 -v 20 --cc_line_size 8192 --stop_on_errors -l test_ddr_02.log",
                "stressapptest -s 600 -f /tmp/file1 -f /tmp/file2 -M 100 -l test_ddr_03.log"],
            "keyWord": "Status: PASS - please verify no corrected errors",
            "ReadUntil": "root@eero-test:/#",
            "Time": 1000,
            "test_name": "ram_stress",
            "FUNC_NAME": "TelnetReadUntilKeyProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 3000,
            "MULTI": false,
            "IP": "192.168.1.1",
            "command": ["stress --cpu 8 --io 4 --vm 2 --vm-bytes 128M --timeout 1800s"],
            "keyWord": "successful run completed in",
            "ReadUntil": "root@eero-test:/#",
            "Time": 2000,
            "test_name": "cpu_stress",
            "FUNC_NAME": "TelnetReadUntilKeyProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 300,
            "MULTI": false,
            "IP": "192.168.1.1",
            "ItemNames": ["emmc_speed_read", "emmc_speed_write"],
            "command": "test-emmc",
            "KeyWord": ["Reading", "Writing"],
            "Block": ["0x00", "0x00"],
            "ReadUntil": "root@eero-test:/#",
            "Time": 20,
            "test_name": "MMC_READ_WRITE_SPEED_TEST",
            "FUNC_NAME": "MMC_WR_SPEED_PROXY"
        },
        {
            "FLAG": true,
            "TIME_OVER": 10,
            "MULTI": false,
            "FAIL_CONTNIUE": true,
            "IP": "192.168.1.1",
            "command": "cat /sys/devices/virtual/thermal/thermal_zone1/temp",
            "Regex": "^[0-9]+$",
            "test_name": "temp_cpu1",
            "FUNC_NAME": "CheckCommandTelnetProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 30,
            "MULTI": false,
            "ALWAYS_RUN": false,
            "host": "192.168.1.10",
            "user": "admin",
            "password": "1234",
            "Times": 1,
            "Delay": 1,
            "Command": ["off"],
            "test_name": "ReleasePowerSwitchOutlet",
            "FUNC_NAME": "PowerSwitchProxy"
        }

    ],
    "END": [
        {
            "FLAG": true,
            "TIME_OVER": 30,
            "MULTI": true,
            "BaseKeys": ["error_details", "status", "finish_time",
                "test_software_version", "start_time", "station_type",
                "error_code", "serial", "mode", "station_name", "position"],
            "TestKeys": ["upper_limit", "lower_limit", "status", "finish_time", "test_name",
                "error_code", "start_time", "test_value", "units"],
            "localFile": "python/log",
            "ElementName": ["mlbsn", "station_name", "mode"],
            "test_name": "CreateJsonLog",
            "FUNC_NAME": "CreateJsonApiProxy" 
        },
        {
            "FLAG": true,
            "TIME_OVER": 30,
            "MULTI": false,
            "localFile": "python/log",
            "ElementName": ["mlbsn", "station_name", "mode", "serial"],
            "test_name": "CreateTxtAndZipLog",
            "FUNC_NAME": "CreateTxtProxy" 
        },
        {
            "FLAG": true,
            "TIME_OVER": 5,
            "WAIT_MULTI_DONE": true,
            "User": "oper",
            "Password": "mfg-oper",
