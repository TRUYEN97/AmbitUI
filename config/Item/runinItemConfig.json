            "LocalName": ["mlbsn", "station_name", "mode", "serial"],
            "LocalType": ".txt",
            "test_name": "UpFTP",
            "FUNC_NAME": "UpFTPProxy"
        },
        {
            "FLAG": true,
            "TIME_OVER": 30,
            "MULTI": false,
            "FAIL_CONTNIUE":true,
            "Command": "cd python && eero_API_client.py log/",
